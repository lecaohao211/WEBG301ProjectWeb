{% extends 'base.html.twig' %}

{% block title %}Food{% endblock %}

{% block body %}
    <h1>Food</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ food.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ food.name }}</td>
        </tr>
        <tr>
            <th>Price</th>
            <td>{{ food.price }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_food_index') }}">Back To List</a>



{% endblock %}
